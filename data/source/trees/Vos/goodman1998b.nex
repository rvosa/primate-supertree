BEGIN TAXA;
[Callithrix_mauesi is unknown to MSW, was pruned from the tree.
Aotus_azarae wsa changed to the preferred Aotus_azarai.]
	DIMENSIONS NTAX = 15;
	TAXLABELS
		Aotus_azarai
		Callithrix_jacchus
		Callithrix_pygmaea
		Callithrix_argentata
		Cebus_albifrons
		Saimiri_sciureus
		Ateles_belzebuth
		Cacajao_calvus
		Hylobates_lar
		Homo_sapiens
		Pongo_pygmaeus
		Otolemur_crassicaudatus
		Daubentonia_madagascariensis
