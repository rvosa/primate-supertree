	DIMENSIONS NTAX = 24;
	TAXLABELS
		Homo_sapiens
		Mandrillus_sphinx
		Papio_hamadryas
		Theropithecus_gelada
		Macaca_sylvanus
		Macaca_fascicularis
		Macaca_fuscata
