#NEXUS


BEGIN TAXA;
[Pruned M. ravelobensis (unknown to MSW). Collapsed haplotypes.
Changed Mirza_coquereli to Microcebus_coquereli.]
	DIMENSIONS NTAX = 7;
	TAXLABELS
		Daubentonia_madagascariensis
		Allocebus_trichotis
		Microcebus_coquereli
		Microcebus_rufus
		Microcebus_murinus
		Cheirogaleus_medius
		Cheirogaleus_major
		;
ENDBLOCK;


BEGIN TREES;
[Tree based on Pastorini et al. (2001, Mol. Phylogenet. Evol.) fig 2c. ML tree based on concatenated COIII, ND3, ND4L and
ND4; and the tRNAs: GLY, ARG, HIS, SER and LEU.]
	TRANSLATE
		1	Daubentonia_madagascariensis,
		2	Allocebus_trichotis,
		3	Microcebus_coquereli,
		4	Microcebus_rufus,
		5	Microcebus_murinus,
		6	Cheirogaleus_medius,
		7	Cheirogaleus_major
		;
	TREE * pastorini_etal_2001fig2 = (1,((2,(3,(4,5))),(6,7)));
ENDBLOCK;
