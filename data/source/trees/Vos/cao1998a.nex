	TAXLABELS
		Hylobates_lar
		Pongo_pygmaeus
		Gorilla_gorilla
		Homo_sapiens
		Pan_troglodytes
		Chlorocebus_aethiops
		Papio_hamadryas
		Macaca_mulatta
		Cercocebus_galeritus
