		2	Hapalemur_griseus,
		3	Eulemur_fulvus,
		4	Varecia_variegata,
		5	Cheirogaleus_major,
		6	Propithecus_tattersalli,
		7	Daubentonia_madagascariensis,
		8	Nycticebus_coucang,
		9	Otolemur_crassicaudatus,
		10	Homo_sapiens
		;
	TREE * yoder_etal_1996fig4 = (10,((7,(5,6,(4,(3,(1,2))))),(8,9)));
ENDBLOCK;
