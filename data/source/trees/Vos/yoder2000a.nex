		1	Microcebus_murinus,
		2	Microcebus_rufus,
		3	Eulemur_macaco,
		4	Eulemur_rubriventer,
		5	Eulemur_fulvus,
		6	Eulemur_mongoz
		;
	TREE * yoder_etal_2000fig3 = ((1,2),(3,(4,(5,6))));
ENDBLOCK;
