	TAXLABELS
		Saimiri_sciureus
		Hylobates_lar
		Pongo_pygmaeus
		Gorilla_gorilla
		Homo_sapiens
		Pan_troglodytes
		Pan_paniscus
		Cheirogaleus_major
		Hapalemur_griseus
		Lemur_catta
		Otolemur_crassicaudatus
		Nycticebus_coucang
		;
ENDBLOCK;


BEGIN TREES;
