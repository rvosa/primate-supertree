#NEXUSBEGIN TAXA;[Taxon names unchanged]	DIMENSIONS NTAX = 5;	TAXLABELS		Cebus_apella		Cebus_olivaceus		Cebus_albifrons		Cebus_capucinus		purvis_mrp_outgroup		;ENDBLOCK;BEGIN TREES;[Tree shape based on Dutrillaux (1988). Cladistic karyological. He uses C. nigrivittatus, not C. olivaceus, but the former is listed in brackets under the latter by Corbet and Hill (1991).]	TRANSLATE		1	Cebus_apella,		2	Cebus_olivaceus,		3	Cebus_albifrons,		4	Cebus_capucinus,		5	purvis_mrp_outgroup		;	TREE * Cebus2 = (5,(1,(2,(3,4))));ENDBLOCK;