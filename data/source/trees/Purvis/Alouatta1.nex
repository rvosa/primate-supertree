#NEXUSBEGIN TAXA;[original data contained A. villosa; preferred synonym in MSW is A. pigra]	DIMENSIONS NTAX = 7;	TAXLABELS		Alouatta_belzebul		Alouatta_caraya		Alouatta_fusca		Alouatta_palliata		Alouatta_seniculus		Alouatta_pigra		purvis_mrp_outgroup		;ENDBLOCK;BEGIN TREES;[Tree shape derived from a one-column MRP matrix meant "just to define outgroup".]	TRANSLATE		1	Alouatta_belzebul,		2	Alouatta_caraya,		3	Alouatta_fusca,		4	Alouatta_palliata,		5	Alouatta_seniculus,		6	Alouatta_pigra,		7	purvis_mrp_outgroup		;	TREE * Alouatta = (7,(1,2,3,4,5,6));ENDBLOCK;