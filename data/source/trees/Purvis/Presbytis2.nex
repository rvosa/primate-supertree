#NEXUSBEGIN TAXA;[Changes:Introduced genera Trachypithecus and Semnopithecus. Purvisrefers to all these taxa as "Presbytis".]	DIMENSIONS NTAX = 14;	TAXLABELS		purvis_mrp_outgroup		Presbytis_comata		Trachypithecus_cristatus		Semnopithecus_entellus		Trachypithecus_francoisi		Presbytis_frontata		Trachypithecus_johnii		Presbytis_melalophos		Trachypithecus_obscurus		Trachypithecus_phayrei		Trachypithecus_pileatus		Presbytis_potenziani		Presbytis_rubicunda		Trachypithecus_vetulus		;ENDBLOCK;BEGIN TREES;[Tree based on Schultz (1986). "Presented without evidence. Also, Schultz has senex not vetulus, and aygula not comata. I {Purvis} have based the substitutions on Corbet and Hill (1991)." Subsequently renamed following MSW.]	TRANSLATE		1	purvis_mrp_outgroup,		2	Presbytis_comata,		3	Trachypithecus_cristatus,		4	Semnopithecus_entellus,		5	Trachypithecus_francoisi,		6	Presbytis_frontata,		7	Trachypithecus_johnii,		8	Presbytis_melalophos,		9	Trachypithecus_obscurus,		10	Trachypithecus_phayrei,		11	Trachypithecus_pileatus,		12	Presbytis_potenziani,		13	Presbytis_rubicunda,		14	Trachypithecus_vetulus		;	TREE * Presbytis2 = (1,4,((2,6,8,13),((5,12,(3,11,(9,10))),(7,14))));ENDBLOCK;