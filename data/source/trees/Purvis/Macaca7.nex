#NEXUSBEGIN TAXA;[Purvis uses Macaca_maurus instead of M._maura (the two are synonymous)and M._tonkeanna instead of M._tonkeana (also synonymous)]	DIMENSIONS NTAX = 4;	TAXLABELS		Macaca_nemestrina		Macaca_fuscata		Macaca_mulatta		purvis_mrp_outgroup		;ENDBLOCK;BEGIN TREES;[Tree shape based on Nakamura et al. (1985). Cladistic analysis of amino acid sequence of fibrinopeptides.]	TRANSLATE		1	Macaca_nemestrina,		2	Macaca_fuscata,		3	Macaca_mulatta,		4	purvis_mrp_outgroup		;	TREE * Macaca7 = (4,(1,(2,3)));ENDBLOCK;