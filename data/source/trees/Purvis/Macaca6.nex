#NEXUSBEGIN TAXA;[Purvis uses Macaca_maurus instead of M._maura (the two are synonymous)and M._tonkeanna instead of M._tonkeana (also synonymous)]	DIMENSIONS NTAX = 5;	TAXLABELS		Macaca_sylvanus		Macaca_fascicularis		Macaca_fuscata		Macaca_mulatta		purvis_mrp_outgroup		;ENDBLOCK;BEGIN TREES;[Tree shape based on Hayasaka et al. (1988b; pp626-644). NJ tree of mtDNA.]	TRANSLATE		1	Macaca_sylvanus,		2	Macaca_fascicularis,		3	Macaca_fuscata,		4	Macaca_mulatta,		5	purvis_mrp_outgroup		;	TREE * Macaca6 = (5,(1,(2,(3,4))));ENDBLOCK;