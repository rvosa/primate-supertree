#NEXUSBEGIN TAXA;[Chlorocebus aethiops originally referred to as Cercopithecus aethiops.]	DIMENSIONS NTAX = 6;	TAXLABELS		Chlorocebus_aethiops		Erythrocebus_patas		Cercopithecus_solatus		Cercopithecus_lhoesti		Cercopithecus_preussi		purvis_mrp_outgroup		;ENDBLOCK;BEGIN TREES;[Tree shape based on Dutrillaux et al. (1988). Cladistic karyological.]	TRANSLATE		1	Chlorocebus_aethiops,		2	Erythrocebus_patas,		3	Cercopithecus_solatus,		4	Cercopithecus_lhoesti,		5	Cercopithecus_preussi,		6	purvis_mrp_outgroup		;	TREE * Cercopithecini2 = (6,(1,2,(3,(4,5))));ENDBLOCK;