#NEXUSBEGIN TAXA;[Purvis uses Macaca_maurus instead of M._maura (the two are synonymous)and M._tonkeanna instead of M._tonkeana (also synonymous)]	DIMENSIONS NTAX = 8;	TAXLABELS		Macaca_nemestrina		Macaca_arctoides		Macaca_radiata		Macaca_fascicularis		Macaca_cyclopis		Macaca_fuscata		Macaca_mulatta		purvis_mrp_outgroup		;ENDBLOCK;BEGIN TREES;[Tree shape based on Melnick and Kidd (1985), given in Fa (1989). Phenetic analysis of plood protein data, no assumption of rate constancy.]	TRANSLATE		1	Macaca_nemestrina,		2	Macaca_arctoides,		3	Macaca_radiata,		4	Macaca_fascicularis,		5	Macaca_cyclopis,		6	Macaca_fuscata,		7	Macaca_mulatta,		8	purvis_mrp_outgroup		;	TREE * Macaca3 = (8,(1,(2,3),(4,(5,6,7))));ENDBLOCK;