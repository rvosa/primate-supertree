#NEXUSBEGIN TAXA;[No taxonomic issues.]	DIMENSIONS NTAX = 5;	TAXLABELS		Pithecia_pithecia		Pithecia_aequatorialis		Pithecia_albicans		Pithecia_irrorata		Pithecia_monachus		;ENDBLOCK;BEGIN TREES;[Tree shape based on Hershkovitz (1987) taxonomy. Splits genus into two groups but no polarity so unrooted tree.]	TRANSLATE		1	Pithecia_pithecia,		2	Pithecia_aequatorialis,		3	Pithecia_albicans,		4	Pithecia_irrorata,		5	Pithecia_monachus		;	TREE * Pithecia2 = (1,(2,3,4,5));ENDBLOCK;