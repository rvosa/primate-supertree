#NEXUSBEGIN TAXA;[Purvis uses Macaca_maurus instead of M._maura (the two are synonymous)and M._tonkeanna instead of M._tonkeana (also synonymous)]	DIMENSIONS NTAX = 14;	TAXLABELS		purvis_mrp_outgroup		Macaca_arctoides		Macaca_assamensis		Macaca_radiata		Macaca_sinica		Macaca_fascicularis		Macaca_cyclopis		Macaca_fuscata		Macaca_mulatta		Macaca_nemestrina		Macaca_maura		Macaca_nigra		Macaca_ochreata		Macaca_tonkeana		;ENDBLOCK;BEGIN TREES;[Tree shape based on Fooden and Lanyon (1989). Jacknife strict-consensus trees of Fitch-Margoliash analyses of blood-proteins. Fig 5b is invalid due to use of an outgroup that may be an ingroup.]	TRANSLATE		1	purvis_mrp_outgroup,		2	Macaca_arctoides,		3	Macaca_assamensis,		4	Macaca_radiata,		5	Macaca_sinica,		6	Macaca_fascicularis,		7	Macaca_cyclopis,		8	Macaca_fuscata,		9	Macaca_mulatta,		10	Macaca_nemestrina,		11	Macaca_maura,		12	Macaca_nigra,		13	Macaca_ochreata,		14	Macaca_tonkeana		;	TREE * Macaca2 = (1,((10,(8,(2,(3,(4,5))),(6,(7,9)))),(11,14),(12,13)));ENDBLOCK;