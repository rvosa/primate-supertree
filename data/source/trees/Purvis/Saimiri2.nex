#NEXUSBEGIN TAXA;[No taxonomic issues.]	DIMENSIONS NTAX = 4;	TAXLABELS		Saimiri_boliviensis		Saimiri_oerstedii		Saimiri_sciureus		Saimiri_ustus		;ENDBLOCK;BEGIN TREES;[Tree based on Hershkovitz (1984). Splits genus into two groups but no polarity - hence unrooted tree.]	TRANSLATE		1	Saimiri_boliviensis,		2	Saimiri_oerstedii,		3	Saimiri_sciureus,		4	Saimiri_ustus		;	TREE * Saimiri2 = (1,(2,3,4));ENDBLOCK;