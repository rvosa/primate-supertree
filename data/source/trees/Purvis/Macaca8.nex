#NEXUSBEGIN TAXA;[Purvis uses Macaca_maurus instead of M._maura (the two are synonymous)and M._tonkeanna instead of M._tonkeana (also synonymous)]	DIMENSIONS NTAX = 8;	TAXLABELS		Macaca_nemestrina		Macaca_fuscata		Macaca_arctoides		Macaca_radiata		Macaca_fascicularis		Macaca_cyclopis		Macaca_mulatta		purvis_mrp_outgroup		;ENDBLOCK;BEGIN TREES;[Tree shape based on Nozawa et al (1977, Jpn J Genet 52,15-30). Phenetic analysis of 26-27 blood proteins.]	TRANSLATE		1	Macaca_nemestrina,		2	Macaca_fuscata,		3	Macaca_arctoides,		4	Macaca_radiata,		5	Macaca_fascicularis,		6	Macaca_cyclopis,		7	Macaca_mulatta,		8	purvis_mrp_outgroup		;	TREE * Macaca8 = (8,(1,(2,((3,4),(5,(6,7))))));ENDBLOCK;